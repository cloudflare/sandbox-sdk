{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",

  "google_auth": false,

  "disabled_agents": [
    "frontend-ui-ux-engineer",
    "document-writer",
    "multimodal-looker"
  ],

  "agents": {
    "explore": {
      "model": "anthropic/claude-haiku-4-5"
    },
    "oracle": {
      "prompt_append": "This is the Cloudflare Sandbox SDK. Key docs: docs/ARCHITECTURE.md, docs/CONCURRENCY.md, docs/SESSION_EXECUTION.md. Focus on 4-layer concurrency model and session.ts complexity."
    }
  },

  "ralph_loop": {
    "enabled": true
  },

  "experimental": {
    "dynamic_context_pruning": {
      "enabled": true
    }
  }
}
