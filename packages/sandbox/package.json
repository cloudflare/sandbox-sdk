{
  "name": "@cloudflare/sandbox",
  "version": "0.6.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudflare/sandbox-sdk"
  },
  "description": "A sandboxed environment for running commands",
  "type": "module",
  "bin": {
    "sandbox": "./dist/cli/index.js"
  },
  "dependencies": {
    "@cloudflare/containers": "^0.0.30",
    "env-paths": "^3.0.0"
  },
  "devDependencies": {
    "@openai/agents": "^0.3.3",
    "@repo/shared": "*"
  },
  "peerDependencies": {
    "@openai/agents": "^0.3.3"
  },
  "peerDependenciesMeta": {
    "@openai/agents": {
      "optional": true
    }
  },
  "tags": [
    "sandbox",
    "codegen",
    "containers",
    "cloudflare",
    "durable objects"
  ],
  "scripts": {
    "build": "rm -rf dist && tsdown --config tsdown.config.ts",
    "check": "biome check && npm run typecheck",
    "fix": "biome check --fix && npm run typecheck",
    "typecheck": "tsc --noEmit",
    "docker:local": "cd ../.. && docker build -f packages/sandbox/Dockerfile --target default --platform linux/amd64 --build-arg SANDBOX_VERSION=$npm_package_version -t cloudflare/sandbox-test:$npm_package_version . && docker build -f packages/sandbox/Dockerfile --target python --platform linux/amd64 --build-arg SANDBOX_VERSION=$npm_package_version -t cloudflare/sandbox-test:$npm_package_version-python .",
    "test": "vitest run --config vitest.config.ts \"$@\"",
    "test:e2e": "cd ../../tests/e2e/test-worker && ./generate-config.sh && cd ../../.. && vitest run --config vitest.e2e.config.ts \"$@\""
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./openai": {
      "types": "./dist/openai/index.d.ts",
      "import": "./dist/openai/index.js",
      "require": "./dist/openai/index.js"
    },
    "./bridge": {
      "types": "./dist/bridge/index.d.ts",
      "import": "./dist/bridge/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js"
    }
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "files": [
    "dist",
    "Dockerfile",
    "startup.sh",
    "README.md",
    "LICENSE"
  ]
}
