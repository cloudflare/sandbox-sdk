{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext", "dom", "dom.iterable"],
    "module": "esnext",
    "jsx": "react-jsx",
    "moduleResolution": "bundler",
    "types": ["@types/node", "./worker-configuration.d.ts"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true
  },
  "include": [
    "worker-configuration.d.ts",
    "worker/**/*.ts",
    "src/**/*.ts",
    "src/**/*.tsx"
  ]
}
