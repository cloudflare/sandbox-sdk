{
	"compilerOptions": {
		"target": "esnext",
		"lib": ["esnext"],
		"module": "esnext",
		"moduleResolution": "bundler",
		"types": ["@cloudflare/workers-types", "./worker-configuration.d.ts"],
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"noEmit": true
	},
	"include": ["worker-configuration.d.ts", "src/**/*.ts"]
}
